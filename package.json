{
  "name": "darwin.js",
  "version": "1.0.0",
  "description": "A genetic algorithm library",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "tslint --project \"./tsconfig.json\"",
    "test": "jest --color --coverage --notify"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],

    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoahBres/Darwin.js.git"
  },
  "words": [
    "genetic algorithm",
    "genetic",
    "typescript",
    "darwin"
  ],
  "author": "Noah Bresler <noahbres@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NoahBres/Darwin.js/issues"
  },
  "homepage": "https://github.com/NoahBres/Darwin.js#readme",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.9",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.1.6"
  }
}
